{
  "name": "GitGuard",
  "url": "https://github.com/Ava-Prime/gitguard",
  "hook_attributes": {
    "url": "https://YOUR-TUNNEL-DOMAIN/webhook"
  },
  "redirect_url": "https://github.com/Ava-Prime/gitguard",
  "public": false,
  "default_permissions": {
    "metadata": "read",
    "contents": "read",
    "pull_requests": "write",
    "checks": "write",
    "statuses": "write"
  },
  "default_events": [
    "pull_request",
    "push",
    "check_suite",
    "check_run",
    "issue_comment",
    "repository"
  ]
}
