{
  "name": "GitGuard",
  "description": "🛡️ AI-powered repository governance that scales with your team. Zero-config security, org-brain intelligence, and transparent policy automation with full audit trails.",
  "homepage": "https://codessa-platform.github.io/gitguard",
  "topics": [
    "git",
    "automation",
    "policy-engine",
    "opa",
    "rego",
    "github-app",
    "code-review",
    "security",
    "governance",
    "temporal",
    "workflow",
    "monitoring",
    "grafana",
    "prometheus",
    "docker",
    "python",
    "fastapi",
    "postgresql",
    "nats",
    "jetstream",
    "org-brain",
    "ai-powered",
    "transparency",
    "devops",
    "ci-cd",
    "repository-management",
    "pull-request",
    "merge-automation",
    "risk-assessment",
    "compliance",
    "audit-trail",
    "decision-engine",
    "ownership-mapping",
    "expertise-routing",
    "zero-config",
    "instant-deployment",
    "github-pages",
    "documentation",
    "mcp-integration",
    "claude-desktop"
  ],
  "license": "MIT",
  "private": false,
  "has_issues": true,
  "has_projects": true,
  "has_wiki": false,
  "has_pages": true,
  "has_downloads": true,
  "archived": false,
  "disabled": false,
  "visibility": "public",
  "default_branch": "main",
  "allow_squash_merge": true,
  "allow_merge_commit": false,
  "allow_rebase_merge": true,
  "allow_auto_merge": true,
  "delete_branch_on_merge": true,
  "allow_update_branch": true,
  "use_squash_pr_title_as_default": true,
  "squash_merge_commit_title": "PR_TITLE",
  "squash_merge_commit_message": "PR_BODY",
  "merge_commit_title": "PR_TITLE",
  "merge_commit_message": "PR_BODY",
  "security_and_analysis": {
    "secret_scanning": {
      "status": "enabled"
    },
    "secret_scanning_push_protection": {
      "status": "enabled"
    },
    "dependabot_security_updates": {
      "status": "enabled"
    },
    "private_vulnerability_reporting": {
      "status": "enabled"
    }
  },
  "social_preview": {
    "image_url": "https://raw.githubusercontent.com/codessa-platform/gitguard/main/docs/images/social-preview.svg",
    "alt_text": "GitGuard - AI-powered repository governance with zero-config security, org-brain intelligence, and transparent policy automation"
  },
  "custom_properties": {
    "deployment_type": "github-app",
    "primary_language": "python",
    "framework": "fastapi",
    "database": "postgresql",
    "message_queue": "nats-jetstream",
    "monitoring": "prometheus-grafana",
    "orchestration": "temporal",
    "policy_engine": "opa-rego",
    "documentation": "github-pages",
    "containerization": "docker",
    "ai_integration": "mcp-claude",
    "deployment_time": "60-seconds",
    "setup_complexity": "zero-config",
    "transparency_level": "full-audit-trail"
  },
  "branch_protection": {
    "main": {
      "required_status_checks": {
        "strict": true,
        "contexts": [
          "ci/tests",
          "ci/lint",
          "ci/security-scan",
          "ci/performance",
          "docs/build"
        ]
      },
      "enforce_admins": false,
      "required_pull_request_reviews": {
        "required_approving_review_count": 1,
        "dismiss_stale_reviews": true,
        "require_code_owner_reviews": true,
        "require_last_push_approval": false
      },
      "restrictions": null,
      "allow_force_pushes": false,
      "allow_deletions": false,
      "block_creations": false,
      "required_conversation_resolution": true,
      "lock_branch": false,
      "allow_fork_syncing": true
    }
  },
  "environments": [
    {
      "name": "production",
      "protection_rules": [
        {
          "type": "required_reviewers",
          "reviewers": ["maintainers"]
        },
        {
          "type": "wait_timer",
          "minutes": 5
        }
      ]
    },
    {
      "name": "staging",
      "protection_rules": [
        {
          "type": "required_reviewers",
          "reviewers": ["developers"]
        }
      ]
    },
    {
      "name": "development",
      "protection_rules": []
    }
  ],
  "labels": [
    {
      "name": "🚀 enhancement",
      "color": "0e8a16",
      "description": "New feature or request"
    },
    {
      "name": "🐛 bug",
      "color": "d73a4a",
      "description": "Something isn't working"
    },
    {
      "name": "📚 documentation",
      "color": "0075ca",
      "description": "Improvements or additions to documentation"
    },
    {
      "name": "🔒 security",
      "color": "b60205",
      "description": "Security-related issue or enhancement"
    },
    {
      "name": "⚡ performance",
      "color": "fbca04",
      "description": "Performance improvement"
    },
    {
      "name": "🧠 org-brain",
      "color": "8b5cf6",
      "description": "Related to org-brain intelligence features"
    },
    {
